{
  "name": "@caju/e2e",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "cypress run",
    "test:sso:open": "cypress open --config baseUrl=http://localhost:5000",
    "test:sso": "cypress run --config baseUrl=http://localhost:5000 --spec 'test/sso/**/*'",
    "test:manager:open": "cypress open --config baseUrl=http://localhost:5001",
    "test:manager": "cypress run --config baseUrl=http://localhost:5001 --spec 'test/manager/**/*'"
  },
  "dependencies": {
    "cypress": "^13.15.1",
    "cypress-xpath": "^2.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "cross-env": "^7.0.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "packageManager": "yarn@3.2.1"
}
